{
  "manifest_version": 3,
  "name": "fonted",
  "version": "1.0",
  "description": "点击切换字体sans-serif",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "点击切换字体sans-serif"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16":  "icons/icon16.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
